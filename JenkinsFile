pipeline{
    agent any
    stages{
        stage('First stage'){
            steps{
                sh 'echo "My stage ONE"'
            }
        }
        stage('Second Satage'){
            steps{
                sh 'echo "My stage TWO"'
            }
        }
        stage('Third Satage'){
            steps{
                sh 'echo "My stage THREE"'
            }
        }
        stage('Final stage'){
            steps{
                sh 'echo "My final stage"'
            }
        }
    }
}